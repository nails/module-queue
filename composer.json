{
    "name": "nails/module-queue",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "nails/common": "dev-feature/pre-new-admin",
        "nails/module-console": "dev-feature/pre-new-admin",
        "nails/module-cron": "dev-feature/pre-new-admin"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "2.*",
        "nails/module-admin": "dev-feature/pre-new-admin"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan analyse -c .phpstan/config.neon"
    },
    "autoload": {
        "psr-4": {
            "Nails\\Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Queue\\": "tests/"
        }
    },
    "suggest": {
        "ext-pcntl": "Required for graceful signal handling in console commands."
    },
    "extra": {
        "nails": {
            "moduleName": "queue",
            "type": "module",
            "namespace": "Nails\\Queue\\",
            "autoload": {
            },
            "data": {
                "nails/module-admin": {
                    "autoload": {
                        "assets": {
                            "js": [
                            ],
                            "css": [
                            ]
                        }
                    }
                }
            }
        }
    }
}
